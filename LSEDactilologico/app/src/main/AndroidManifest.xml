<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.NicolasFernandez.lsedactilologico">
    <uses-permission android:name="android.permission.RECORD_AUDIO"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/appicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/appicon_round"
        android:supportsRtl="true"
        android:theme="@style/CENECMalaga">
        <activity android:name="es.cenecmalaga.lsedactilologico.actividades.AyudanosAMejorar"></activity>
        <activity android:name="es.cenecmalaga.lsedactilologico.actividades.ActividadSobreNosotros" />
        <activity android:name="es.cenecmalaga.lsedactilologico.actividades.ActividadTraductor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>